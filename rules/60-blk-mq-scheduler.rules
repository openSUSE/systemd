# do not edit this file, it will be overwritten on update

ACTION!="add", GOTO="blk_mq_scheduler_end"
SUBSYSTEM!="block", GOTO="blk_mq_scheduler_end"
TEST!="mq", GOTO="blk_mq_scheduler_end"

# use bfq under blk-mq for rotational devices (except for host-managed SMR
# drives) and for SSDs
KERNEL=="sd*[!0-9]", ATTR{queue/zoned}!="host-managed", ATTR{queue/scheduler}="bfq"

LABEL="blk_mq_scheduler_end"
